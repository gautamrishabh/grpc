// LONGEST SUM-ARRAY SUM


// 1 //SIMPLE METHOD
// TIME COMPLEXITY = O(n^3)
#include <iostream>
#include <math.h>
#include <algorithm>

// g++ hello.cpp && ./a.out
using namespace std;
//#include<bits/stdc++.h> 

int main () {
	//int arr[] = {-3, 2, -4, -1, 2, 3, -2};
	int arr[] = {1, 4, -2, 4, -1, 3, 5, -6};
	int n = 8;
	int ans = INT_MIN;
	for(int i = 0; i < n; i++) {
		for(int j = i; j < n; j++) {
			int sum = 0;
			for (int k = i; k <= j; k++) {
				sum += arr[k];
				if (sum > ans) {
					ans = sum;
				}
			}
		}
	}
	cout << ans;
	return 0;
}
